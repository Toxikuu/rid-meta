#!/bin/bash
NAME="xorgproto"
VERS="${NAME}_version"; VERS="${!VERS}"
LINK="https://xorg.freedesktop.org/archive/individual/proto/xorgproto-$VERS.tar.xz"
UPST="https://repology.org/project/xorgproto/information"
DEPS="util_macros"


IDIR=$(cat << '~fin.'

mn
mv -v /usr/share/doc/xorgproto{,-2024.1}

~fin.
)

RDIR=$(cat << '~fin.'

rm -vf /usr/share/doc/xorgproto-2024.1/

~fin.
)

UDIR=$(cat << '~fin.'
~fin.
)
