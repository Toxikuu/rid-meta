#!/bin/bash
NAME="xbitmaps"
VERS="1.1.3"
LINK="https://www.x.org/pub/individual/data/$NAME-$VERS.tar.xz"
UPST="https://archlinux.org/packages/extra/any/xbitmaps/" # the actual upstream is annoying to parse
DEPS="util-macros"


IDIR=$(cat << '~fin.'

./configure $XORG_CONFIG
make install

~fin.
)

RDIR=$(cat << '~fin.'

rm -vf /usr/include/X11/bitmaps

~fin.
)

UDIR=$(cat << '~fin.'
~fin.
)
