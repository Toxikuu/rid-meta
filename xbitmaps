#!/bin/bash
NAME="xbitmaps"
VERS="${NAME}_version"; VERS="${!VERS}"
LINK="https://www.x.org/pub/individual/data/$NAME-$VERS.tar.xz"
DEPS="util_macros"


IDIR=$(cat << '~fin.'

./configure $XORG_CONFIG
make install

~fin.
)

RDIR=$(cat << '~fin.'

rm -vf /usr/include/X11/bitmaps

~fin.
)

UDIR=$(cat << '~fin.'
~fin.
)
