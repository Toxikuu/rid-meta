DEPS="libpciaccess"


idir() {

mn   \
    -D udev=true          \
    -D valgrind=disabled  \
    -D cairo-tests=disabled

32mn \
    -D udev=true          \
    -D valgrind=disabled  \
    -D cairo-tests=disabled

}

rdir() {

echo not implemented

}
