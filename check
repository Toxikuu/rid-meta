#!/bin/bash
NAME="check"
VERS="0.15.2"
LINK="https://github.com/libcheck/check/releases/download/$VERS/check-$VERS.tar.gz"
UPST="https://github.com/libcheck/check/tags"
DEPS=""
NEWS="If building 32-bit pulseaudio, ensure you have 32-bit check."


idir() {

export docdir=/tmp/rid/trash
cm
32cm
unset docdir

}

rdir() {

rm -vf  /usr/bin/checkmk    \
        /usr/lib{,32}/libcheck.so*

    }


