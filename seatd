#!/bin/bash
NAME="seatd"
VERS="${NAME}_version"; VERS="${!VERS}"
LINK="https://git.sr.ht/~kennylevinsen/seatd/archive/$VERS.tar.gz"
DEPS="elogind"


IDIR=$(cat << '~fin.'

mn

~fin.
)

RDIR=$(cat << '~fin.'

rm -vf /usr/bin/seatd{,-launch} \
       /usr/lib/libseat.so*

~fin.
)

UDIR=$(cat << '~fin.'
~fin.
)
