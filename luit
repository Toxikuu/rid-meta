#!/bin/bash
NAME="luit"
VERS="2.0.20240910"
ver="${VERS##*.}"
LINK="https://invisible-mirror.net/archives/luit/luit-$ver.tgz"
UPST="https://invisible-mirror.net/archives/luit/current/?C=M;O=D"
VCMD="curl -s '$UPST' | grep luit- | head -n1| cut -d- -f2 | cut -d. -f 1-3"
DEPS="libx11"


idir() {

cm

}

rdir() {

rm -vf /usr/bin/luit

}
