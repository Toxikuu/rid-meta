#!/bin/bash
NAME="less"
VERS="${NAME}_version"; VERS="${!VERS}"
LINK="https://www.greenwoodsoftware.com/less/less-$VERS.tar.gz"
UPST="https://github.com/gwsw/less/tags"
DEPS=""


IDIR=$(cat << '~fin.'

cm --sysconfdir=/etc

~fin.
)

RDIR=$(cat << '~fin.'

rm -vf /usr/bin/less{,echo,key}

~fin.
)

UDIR=$(cat << '~fin.'
~fin.
)
